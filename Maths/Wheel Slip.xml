  <MathExpression Id="Slip Long Accel R" DisplayUnit="%" DisplayDPS="2" DisplayColorIndex="5" Interpolate="1" Script="('Slip Long Accel RL' [%] + 'Slip Long Accel RR' [%]) / 2" SampleRate="0" Unit="%"/>
