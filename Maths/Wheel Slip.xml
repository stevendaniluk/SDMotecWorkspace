  <MathExpression Id="Slip Long Accel FR" DisplayUnit="%" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="choose('Slip Long FR' [%] &gt; 0, 'Slip Long FR' [%], 0)
