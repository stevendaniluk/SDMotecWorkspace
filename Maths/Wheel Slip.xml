  <MathExpression Id="Slip Long FR" DisplayUnit="%" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="choose('Speed' [m/s] &gt; 5, 'Wheel Speed FR' [m/s] / 'Speed' [m/s] - 1, 0)" SampleRate="0" Unit="ratio"/>
